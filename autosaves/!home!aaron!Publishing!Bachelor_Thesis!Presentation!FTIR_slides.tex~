\section{Introduction}


\subsection{FTIR spectroscopy}

\begin{frame}
  \frametitle{Infrared radiation}
  
    \begin{tikzpicture}
      \draw[gray] (0,0)  sin (1,2) cos (2,0) sin (3,-2) cos (4,0) sin (5,2) cos (6,0) sin (7,-2) cos (8,0) sin(9,2) cos(10,0) sin (11,-2) ;
      \draw[thick, blue]  (2,0) -- (6,0);
      \path (2, 0) -- (6, 0) node [midway, above] {\huge\alert{$\lambda$}};
      \end{tikzpicture}
      \begin{block}{Wavelength \huge\alert{$\lambda$}\normalfont }
        \begin{itemize}
        \item IR radiation has wavelengths from \si{750  \nano\metre} to \si{1 ~ \milli\metre}
        \end{itemize}
      \end{block}
\end{frame}

\begin{frame}{Infrared radiation - What can it do?}
    \begin{itemize}
    \item It can make things move
    \item What kind of things?
    \item Things, that have rougly the same size as the wavelength \alert{$\Rightarrow$} Molecules (or parts of them)
    \end{itemize}
    \begin{columns}[c]
      \column{0.5 \textwidth}
      \begin{block}{This works}
        \vspace{0.5 cm}
        \begin{tikzpicture}[thick]
          \draw[gray] (0,0)  sin (1,2) cos (2,0) sin (3,-2) cos (4,0);
          \draw[orange] (4,2) -- (4,0.5);
          \draw[orange!10, fill = orange!10] (4.5,0.1) circle [radius = 0.5 cm];
          \draw[orange,fill = orange] (4,0) circle [radius = 0.5 cm];
          \draw[orange!10] (4,2) -- (4.5,0.2);
        \end{tikzpicture}          
      \end{block}
      \column{0.5 \textwidth}

      \begin{block}{This doesn't work}
        \vspace{0.5 cm}
        \begin{tikzpicture}[thick]
          \draw[gray!20] (0,0)  sin (1,2) cos (2,0) sin (3,-2) cos (4,0);
          % The above line is just a workaround to make sure that the block headlines are at the same height
          \path[draw=gray,decorate, decoration=snake] (0,0) -- (5,0);
          \draw[orange] (4,2) -- (4,0.5);
          \draw[orange!10, fill = orange!10] (4.5,0.1) circle [radius = 0.5 cm];
          \draw[orange,fill = orange] (4,0) circle [radius = 0.5 cm];
          \draw[orange!10] (4,2) -- (4.5,0.2);
                \end{tikzpicture}
    \end{block}
      

  \end{columns}
\end{frame}

\begin{frame}
\tikzstyle{na} = [baseline=-.5ex]
  \frametitle{Spectroscopy}
  \centering 
  \begin{columns}
  \column{0.7 \textwidth}
  \begin{itemize}
% Scanning misses --> How do we get a spectrum
  \item A beam is sent through the sample    
  \item A detector detects the intensity of the beam \alert{after} the sample
  \item Knowing the intensity of the beam before the sample, software calculate can absorption or transmission
  \item In the case of infrared spectroscopy, this is done for wavelengths in the infrared range
  \item For better comparability, in IR spectroscopy the \alert{wavenumber $\nu$} in \si{\per\cm} is used rather than the wavelength $\lambda$
  \end{itemize}
    \column{0.3\textwidth}
      \begin{tikzpicture}
    \node[draw = gray,minimum width = 1 cm,minimum height = 5 cm,thick,rectangle](Sample) at (-3,0) {Sample}; 
    \coordinate(start) at (-5,0);
    \coordinate(detect) at (3,0);
    \path[draw=orange,thick,decorate, decoration=snake] (start) -- (Sample);
    \path[draw=orange!30,thick,decorate, decoration=snake] (Sample) -- (detect);
  \end{tikzpicture}

  \end{columns}

  \begin{alertblock}{Remember}
    Some energy of the wave gets transformed into kinetic energy in the sample.
    This transformation is represented as a peak in a spectrum!
  \end{alertblock}
\end{frame}
\begin{frame}
  \frametitle{How do you get a full spectrum from this?}
  \begin{itemize}
  \item You just repeat this measurement for a certain range of wavenumbers
  \item If a certain group of atoms in the sample is moving at one of these wavelengths, you are going to get an absorption value
  \item This absorption value is then mapped to this specific wavelength so a point can be plotted in a coordinate system
  \item If you have many of these points, you can see a spectrum
  \end{itemize}

\end{frame}

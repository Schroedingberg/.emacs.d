@article{ 2007Rubber-CarbonBlack,
Author = {Hu, Weiguo and Ellul, Maria D. and Tsou, Andy H. and Datta, Sudhin},
Title = {{Filler distribution and domain size of elastomer compounds by
   solid-state NMR and AFM}},
Journal = {{RUBBER CHEMISTRY AND TECHNOLOGY}},
Year = {{2007}},
Volume = {{80}},
Number = {{1}},
Pages = {{1-13}},
Month = {{MAR-APR}},
Note = {{Annual Spring Meeting of the
   Rubber-Division-of-the-American-Chemical-Society, San Antonio, TX, MAY
   16-18, 2005}},
Organization = {{Amer Chem Soc, Rubber Div}},
Abstract = {{Solid-state NMR methods were used to characterize the filler
   distribution in rubber blends and domain size of thermoplastic
   vulcanizates (TPV). Correlation between filler content and magic angle
   spinning (MAS) C-13 and H-1 NMR signal linewidth was established, and
   filler content in each component of the rubber blends was determined. In
   IIR/BR and BIMS/BR blends, carbon black enriches in BR phase. Comparison
   of the carbon black enrichment in both blends suggests that BIMS has a
   slightly stronger interaction with fillers than does IIR. Results
   obtained from AFM analysis were compared with NMR observations. Spin
   diffusion NMR was demonstrated to be useful for quantifying the amount
   of interfacial component in TPV. Its advantages - ease of quantification
   and high sensitivity for smaller domains - make it a complementary
   approach to microscopic techniques for elastomer morphology
   characterization.}},
DOI = {{10.5254/1.3548166}},
ISSN = {{0035-9475}},
Unique-ID = {{ISI:000249719900001}},
}

@article{Worley20141,
title = "Simultaneous phase and scatter correction for \{NMR\} datasets ",
journal = "Chemometrics and Intelligent Laboratory Systems ",
volume = "131",
number = "",
pages = "1 - 6",
year = "2014",
note = "",
issn = "0169-7439",
doi = "http://dx.doi.org/10.1016/j.chemolab.2013.11.005",
url = "http://www.sciencedirect.com/science/article/pii/S0169743913002153",
author = "Bradley Worley and Robert Powers",
keywords = "Phase-scatter correction",
keywords = "MSC",
keywords = "SNV",
keywords = "NMR",
keywords = "PCA",
keywords = "PLS ",
abstract = "Abstract Nuclear magnetic resonance (NMR) spectroscopy has proven invaluable in the diverse field of chemometrics due to its ability to deliver information-rich spectral datasets of complex mixtures for analysis by techniques such as principal component analysis (PCA). However, \{NMR\} datasets present a unique challenge during preprocessing due to differences in phase offsets between individual spectra, thus complicating the correction of random dilution factors that may also occur. We show that simultaneously correcting phase and dilution errors in \{NMR\} datasets representative of metabolomics data yields improved cluster quality in \{PCA\} scores space, even with significant initial phase errors in the data. "
}


@article{Chen2002164,
title = "An efficient algorithm for automatic phase correction of \{NMR\} spectra based on entropy minimization ",
journal = "Journal of Magnetic Resonance ",
volume = "158",
number = "1–2",
pages = "164 - 168",
year = "2002",
note = "",
issn = "1090-7807",
doi = "http://dx.doi.org/10.1016/S1090-7807(02)00069-1",
url = "http://www.sciencedirect.com/science/article/pii/S1090780702000691",
author = "Li Chen and Zhiqiang Weng and LaiYoong Goh and Marc Garland",
keywords = "Phase correction",
keywords = "NMR spectra",
keywords = "Entropy minimization",
keywords = "Derivative ",
abstract = "A new algorithm for automatic phase correction of \{NMR\} spectra based on entropy minimization is proposed. The optimal zero-order and first-order phase corrections for a \{NMR\} spectrum are determined by minimizing entropy. The objective function is constructed using a Shannon-type information entropy measure. Entropy is defined as the normalized derivative of the \{NMR\} spectral data. The algorithm has been successfully applied to experimental 1H \{NMR\} spectra. The results of automatic phase correction are found to be comparable to, or perhaps better than, manual phase correction. The advantages of this automatic phase correction algorithm include its simple mathematical basis and the straightforward, reproducible, and efficient optimization procedure. The algorithm is implemented in the Matlab program ACME—Automated phase Correction based on Minimization of Entropy. "
}
                  


@thesis{Lanzl2002,
           month = {March},
           title = {Charakterisierung von Ruß-Kautschuk-Mischungen mittels dielektrischer Spektroskopie},
          author = {Thomas Lanzl},
            year = {2002},
        keywords = {Impedanzspektroskopie , Ruß , Kautschuk , Gemisch , Leitfähigkeit , Dispersion ,  , permittivity , dielectric loss , conductivity , rubber-carbon black compounds , composite materials},
             url = {http://epub.uni-regensburg.de/9907/},
        abstract = {Die Zielsetzung der vorliegenden Arbeit besteht darin, die innere 
Struktur von Ruß-Kautschuk-Mischungen mittels dielektrischer
 Spektroskopie zu charakterisieren. Die genaue Kenntnis der 
räumlichen Anordnung der F{\"u}llerpartikel soll dazu beitragen, 
den Mechanismus der Verstärkung beim F{\"u}llstoff Ruß zu erklären.
Die entscheidende Neuerung hierbei ist eine Modifikation der 
{\"u}blichen dielektrischen Versuchsanordnung. Bei diesem Verfahren 
wird die unbehandelte Probe zwischen zwei zusätzlichen verlustfreien 
Isolierschichten in den Meßkondensator eingebracht. Dadurch wird im 
Unterschied zur {\"u}blichen Methode die Reproduzierbarkeit von 
Meßergebnissen gewährleistet. Durch eine eigens entwickelte 
Modellfunktion läßt sich dieses modifizierte System vollständig 
beschreiben. Eine Auswertung von Meßergebnissen anhand dieser 
Modellfunktion liefert Parameter, die Informationen hinsichtlich 
der inneren Struktur von Mischungen beinhalten. Ein Vergleich der 
Ergebnisse f{\"u}r unterschiedliche Rußsorten zeigt, von welchen 
Einflußgr{\"o}ßen die innere Struktur abhängt.
},
  institution =  {Uni Regensburg},
}


\frame{\titlepage}

\begin{frame}
\frametitle{NMR spectroscopy}
  \begin{columns}[T]
    
\column{0.5\linewidth}
  \begin{block}<1>{What can it be used for?}
      \begin{itemize}
      \item To analyse a number of nuclei, most commonly proton ($^1H$) and carbon ($^{13}C$)
      \item Analysis can be undertaken in both solution and solid
      \item Can be used to identify and analyse the structure of a sample, different parts of the sample will generally give different signals
      \end{itemize}
    \end{block}
\column{0.5\linewidth}
  \begin{alertblock}{Solid-state NMR}
      \begin{itemize}
      \item Powders/granules packed in a rotor
      \item Can be used for samples that are hard to dissolve (e.g. polymers)
      \item Can provide information on mobility of molecules, e.g. if there are small molecules in the sample or how stiff/soft
        the sample is 
      \end{itemize}

    \end{alertblock}
  \end{columns}
\end{frame}

\begin{frame}
  \frametitle{Magic angle spinning MAS}
  \begin{columns}[T]
    \column{0.5\textwidth}
    \begin{block}{What?}
      \begin{itemize}
      \item Sample spins at high frequency
      \item Orientation relative to magnetic field \ang{54.74}
        $\Rightarrow$ \alert{magic angle}
      \end{itemize}
    \end{block}
    \column{0.5\textwidth}
    \begin{tikzpicture}
      \draw[->](1,0)--(1,5);
      \filldraw[fill=mLightBrown!20,draw=mLightBrown!50](1,0)--(1,3)
      arc [start angle=0, end angle=54.74, radius=1]--cycle;
    \end{tikzpicture}
  \end{columns}
\end{frame}

\begin{frame}
  \frametitle{Peak width}
  \begin{columns}[T]
    \column{0.4\textwidth}
    \begin{itemize}
    \item Peak width will \alert{decrease} with \alert{increasing} mobility\cite{Gaborieau2005}
    \item  Mobility can be increased by raising \alert{temperature}
    \end{itemize}
    \begin{block}{Consequence}
      Sharper peaks can be obtained 
      if NMR spectrum is measured at higher temperature
    \end{block}
    \column{.6\textwidth}
    \includegraphics[trim = 0cm 8cm 23cm 25cm
cm, clip, scale = 0.35]{peakwidth.png}
  \end{columns}
\end{frame}


\begin{frame}\frametitle{Types of experiments used }
      \begin{block}{$^1H$ NMR}
        \begin{itemize}
        \item Short experiment (under 5 min)
        \item Information regarding 
        small molecules 
        and rigidity of sample
        \end{itemize}

    \end{block}

    \begin{block}{$^{13}C$ NMR CP (cross polarization)}
      \begin{itemize}
      \item Gives more information (typically several hours)
      \item Information regarding different chemical shifts and allows identification of different functional groups of the sample
      \end{itemize}


    \end{block}

    \begin{block}{$^{13}C$ NMR SPE (single pulse excitation)}
      \begin{itemize}
      \item Takes typically several days
      \item Similar to CP experiment however more sensitive to small molecules
      \item Experiments undertaken depend on type of sample however, CP is generally used in preference to SPE because it is shorter
      \end{itemize}



      
    \end{block}
\end{frame}



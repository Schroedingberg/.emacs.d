   label="Heat\nresistant?"
   ];
   Question4[color=PowderBlue,shape=ellipse,
   label="Chemical\nresistant?"
   ];
}

 addit->Ext_Proc;
 Question1->Ans;
 Question2->Ans;
 Question3->Ans;
 Question4->Ans;
 Adhesive->Question1;
 Adhesive->Question2;
 Adhesive->Question3;
 Adhesive->Question4;
 Ans-> NMR;
 Ans->FTIR;
 Ans->Raman;
 NMR-> Ext_Proc[label=Improve];
 FTIR->Ext_Proc[label=Improve];
 Raman->Ext_Proc[label=Improve];
 BRubber-> Ext_Proc->Adhesive;
// Adhesive->Damage[label="Repair"];
// PlaceA->Damage->Transport_Medium->PlaceB;
  }
